{
	"name": "daytime",
	"version": "1.0.2",
	"description": "combine two Date objects, \"day\" and \"time\", into a new Date object",
	"main": "index.js",
	"scripts": {
		"pretest": "npm run --silent lint",
		"test": "npm run --silent tests-only && npm run --silent coverage-quiet",
		"tests-only": "node test.js",
		"posttest": "npm run audit",
		"coverage": "covert test.js",
		"coverage-quiet": "covert test.js --quiet",
		"lint": "eslint .",
		"preaudit": "npm install --package-lock --package-lock-only",
		"audit": "npm audit",
		"postaudit": "rm package-lock.json"
	},
	"dependencies": {},
	"devDependencies": {
		"@ljharb/eslint-config": "^13.1.1",
		"covert": "^1.1.1",
		"es5-shim": "^4.5.12",
		"eslint": "^5.12.0",
		"tape": "^4.9.2"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/ljharb/daytime.git"
	},
	"keywords": [
		"date",
		"day",
		"time",
		"combine",
		"merge"
	],
	"author": "Jordan Harband",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ljharb/daytime/issues"
	},
	"testling": {
